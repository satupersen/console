{
    "name": "satupersen/console",
    "description": "The Illuminate Console package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "satupersen/contracts": "^1.0",
        "satupersen/support": "^1.0",
        "symfony/console": "^5.1",
        "symfony/process": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Satupersen\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "dragonmantank/cron-expression": "Required to use scheduling component (^2.0).",
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (^6.3|^7.0).",
        "satupersen/filesystem": "Required to use the generator command (^1.0)"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
